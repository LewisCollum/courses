library ieee;
use ieee.std_logic_1164.all;

entity Enabler is
	port(
		i: in std_logic;
		enable: in std_logic;
		o: out std_logic
	);
end entity Enabler;

architecture behavioral of Enabler is
	type choice is array(0 to 1, 0 to 1) of std_logic;
	constant test: choice := (('1', '0'), ('0', '1'));
begin
	process(enable)
	begin
		if enable = test(1, 0) then
			o <= i;
		else 
			o <= 'Z';
		end if;
	end process;
end architecture behavioral;~/repos/SRAM-and-Keypad-to-Seven-Segment-Display/source $ rm Enabler/Enabler.vhd 
~/repos/SRAM-and-Keypad-to-Seven-Segment-Display/source $ ls
