library ieee;
use ieee.std_logic_1164.all;

entity Enabler is
	port(
		i: in std_logic;
		enable: in std_logic;
		o: out std_logic
	);
end entity Enabler;

architecture behavioral of Enabler is
begin
	process(enable)
	begin
		if enable = '1' then
			o <= i;
		else 
			o <= 'Z';
		end if;
	end process;
end architecture behavioral;~/repos/SRAM-and-Keypad-to-Seven-Segment-Display/source/Enabler $ ls
